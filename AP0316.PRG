SAVE SCRE TO SCRE4
store .t. to wsub4
do while wsub4
   RESTORE SCREEN FROM SCRE4
   @ 04,00 clear to 09,23
   @ 04,00       to 09,23
   @ 04,01 SAY    "HISTORICO NOMINAL"
   @ 05,01 PROMPT "NOMINA ORDINARIA      "
   @ 06,01 PROMPT "NOMINA CONCEPTUAL     "
   @ 07,01 PROMPT "NOMINA VERTICAL       "
   @ 08,01 PROMPT "NOMINA HORIZONTAL     "
   MENU TO WSUBSUB4
   DO CASE
   CASE WSUBSUB4 = 1
        DO AP0316A
   CASE WSUBSUB4 = 2
        DO AP0316B
   CASE WSUBSUB4 = 3
        DO AP0316C
   CASE WSUBSUB4 = 4
        DO AP0316D
   CASE WSUBSUB4 = 0
        EXIT
   ENDCASE
enddo
